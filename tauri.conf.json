{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "../build",
    "distDir": "../build",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "My New App",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "dialog": {
        "all": true,
        "open": true,
        "save": true
      },
      "shell": {
        "all": true,
        "open": true
      }
    },
    "cli": {
      "description": "",
      "longDescription": "",
      "beforeHelp": "",
      "afterHelp": "",
      "args": [
        {
          "name": "urlPath",
          "index": 1,
          "takesValue": true
        }
      ]
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.nhatvu148",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "My New App",
        "width": 1400,
        "height": 900,
        "url": ""
      }
    ]
  }
}